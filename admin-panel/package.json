{
  "name": "ai-music-uploader-admin",
  "version": "1.0.0",
  "description": "Modern Admin Panel for AI Music Uploader",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.7.2",
    "node-cron": "^3.0.2",
    "ws": "^8.13.0",
    "child_process": "^1.0.2",
    "fs-extra": "^11.1.1",
    "chokidar": "^3.5.3",
    "systeminformation": "^5.21.7",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "helmet": "^7.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["admin", "dashboard", "ai", "music", "uploader", "react", "nodejs"],
  "author": "AI Music Uploader Team",
  "license": "MIT"
}
