{
  "name": "ai-music-uploader-admin",
  "version": "1.0.0",
  "description": "AI Music Uploader - Complete Node.js Backend with React Admin Panel",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"nodemon server/index.js\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client",
    "docker:build": "docker build -t ai-music-uploader:latest .",
    "docker:start": "./deploy.sh start",
    "docker:stop": "./deploy.sh stop",
    "docker:logs": "./deploy.sh logs",
    "docker:status": "./deploy.sh status"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fluent-ffmpeg": "^2.1.3",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-telegram-bot-api": "^0.63.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "sequelize": "^6.35.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "systeminformation": "^5.21.20",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "ai",
    "music",
    "youtube",
    "uploader",
    "automation",
    "admin-panel"
  ],
  "author": "LT Dev.1",
  "license": "MIT"
}
